root_pass_mysql=$(openssl rsautl -decrypt -inkey /root/private_key.pem -in /root/password_root_mysql.bin 2>/dev/null)
maxscale_pass=$(openssl rsautl -decrypt -inkey /root/private_key.pem -in /root/password_maxscale.bin 2>/dev/null)
user_pass_maxscale=$(openssl rsautl -decrypt -inkey /root/private_key.pem -in /root/password_user_maxscale.bin 2>/dev/null)
password_toto=$(openssl rsautl -decrypt -inkey /root/private_key.pem -in /root/password_toto.bin 2>/dev/null)
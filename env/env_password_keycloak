password=$(openssl rsautl -decrypt -inkey private_key.pem -in password_keycloak.bin 2>/dev/null)
password_toto=$(openssl rsautl -decrypt -inkey private_key.pem -in password_toto.bin 2>/dev/null)
password_base_keycloak=$(openssl rsautl -decrypt -inkey private_key.pem -in password_base_keycloak.bin 2>/dev/null)
root_pass_mysql=$(openssl rsautl -decrypt -inkey /root/private_key.pem -in /root/password_root_mysql.bin 2>/dev/null)